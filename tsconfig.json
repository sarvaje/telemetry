{
  "compilerOptions": {
      "alwaysStrict": true,
      "composite": true,
      "declaration": true,
      "declarationMap": true,
      "inlineSourceMap": true,
      "lib": [
          "dom",
          "dom.iterable",
          "es2017",
          "esnext",
          "esnext.asynciterable"
      ],
      "module": "commonjs",
      "moduleResolution": "node",
      "newLine": "lf",
      "noUnusedLocals": true,
      "outDir": "dist",
      "removeComments": true,
      "target": "es2017",
      "strict": true
  }
}
